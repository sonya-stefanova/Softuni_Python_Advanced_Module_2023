expression = input()

opening_brackets = []

balanced = True

for symbol in expression:

    if symbol in '({[':
        opening_brackets.append(symbol)

    elif not opening_brackets:
        balanced = False
        break

    else:
        last_opening_bracket = opening_brackets.pop()
        if f'{last_opening_bracket}{symbol}' not in '(){}[]':
            balanced = False
            break

if balanced and not opening_brackets:
    print('YES')
else:
    print('NO')
